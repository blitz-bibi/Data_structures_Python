   "source": [
    "def bubble_sort(L):\n",
    "    n = len(L)\n",
    "    for i in range(n):\n",
    "        for j in range(n-i-1):\n",
    "            if L[j] > L[j+1]:\n",
    "                L[j], L[j+1] = L[j+1], L[j]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 20,
   "id": "bb096576",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Before sorted:  [32, 2, 4, 16, 8]\n",
      "After sorted:  [2, 4, 8, 16, 32]\n"
     ]
    }
   ],
   "source": [
    "L = [32,2,4,16,8]\n",
    "print(\"Before sorted: \", L)\n",
    "bubble_sort(L)\n",
    "print(\"After sorted: \", L)"
   ]
