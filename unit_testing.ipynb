    "def average(L):\n",
    "    if not L:\n",
    "        return None\n",
    "    \n",
    "    return sum(L)/len(L)\n",
    "\n",
    "def test_average():\n",
    "    assert 3.0 == average([1,2,3,4,5])"
   ]
